{
    "name": "renoki-co/hej",
    "description": "Hej! is a simple authentication boilerplate for Socialite.",
    "keywords": [
        "laravel",
        "php",
        "social",
        "login",
        "register",
        "socialite",
        "facebook",
        "github",
        "twitter",
        "gitlab",
        "google"
    ],
    "license": "Apache-2.0",
    "homepage": "https://github.com/renoki-co/hej",
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/database": "^9.0.1",
        "illuminate/support": "^9.0.1",
        "laravel/socialite": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "RenokiCo\\Hej\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenokiCo\\Hej\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.3",
        "mockery/mockery": "^1.5",
        "orchestra/database": "^7.0",
        "orchestra/testbench": "^7.0",
        "orchestra/testbench-browser-kit": "^7.0",
        "orchestra/testbench-core": "^7.21",
        "phpunit/phpunit": "^9.5.21"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "RenokiCo\\Hej\\HejServiceProvider"
            ]
        }
    }
}
